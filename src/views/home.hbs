<div class="card mt-2 w-100">
    <div class="card-header bg-dark text-white">
        <h3> <i class="far fa-images"></i>Lista de imagenes</h3>
    </div>
    <div class="card-body">
        <div class="row">
            {{#each images}}
            <div class="col-md-2">
                <a href="/images/{{uniqueId}}">
                    <img src="/public/upload/{{filename}}" alt="" class="w-10 h-10 img-thumbnail">
                </a>
            </div>
            {{/each}}
        </div>
    </div>
</div>